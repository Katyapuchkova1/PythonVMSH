a = float (input ("a = "))
b = float (input ("b = "))
c = float (input ("c = "))

descr = b**2 - 4 * a * c
if descr > 0:
    import math
    x1 = (-b + math.sqrt(descr)) / (2 * a)
    x2 = (-b - math.sqrt(descr)) / 2 / a
    print (x1, x2)
elif descr == 0:
    x = -b / (2 * a)
    print (x)
else:
    print ("Корней нет")
