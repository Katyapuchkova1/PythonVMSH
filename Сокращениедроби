def GCD(a, b):
    
    if a > b:
        x = a
        y = b
    else:
        y = a
        x = b
    while y > 0:
        t = x / y
        x = y
        y = t
    return x

class Rational:
    
    def __init__(self, a, b):
        self.chisl = a
        self.znam = b
        
    def socrat(self):
        print (str(self.chisl/ GCD(self.chisl, self.znam)) + " / " + str(self.znam / GCD(self.chisl, self.znam)))
   
First = Rational(2, 4)
First.socrat()
